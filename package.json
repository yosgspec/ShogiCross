{
  "name": "shogicross",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "run-s build:*",
    "build:sharp": "npx sharp -i ./shogiCross/img/**/*.png -f png -o {dir} ",
    "build:vite": "npx vite build",
    "build:after": "node buildAfter.js ./dist/ShogiCross.js",
    "build:api": "npx documentation build ./shogiCross/ShogiCross/lib.js -f html -o ./shogiCross/doc/api",
    "build:dts": "npx tsc"
  },
  "dependencies": {
    "vite": "5.1.*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sharp-cli": "^4.2.0",
    "typescript": "^5.4.2"
  }
}
